generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "rhel-openssl-1.0.x"]
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model DeployModel {
  id                     String @id @default(uuid()) @map("_id")
  deployModelName        String
  deployModelType        String
  databaseType           String
  executionEnvironment   String
  ownerEmail             String
  backendSourceCodePath  String @default("")
  frontendSourceCodePath String @default("")
  accessKeyIdPath        String @default("")
  secretAccessKeyPath    String @default("")
}
